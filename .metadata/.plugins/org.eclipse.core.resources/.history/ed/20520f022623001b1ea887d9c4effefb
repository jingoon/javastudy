		Scanner sc = new Scanner(System.in);
		
		Command[] coms = {new SelectCommand(),
						new DeleteCommand(),
						new InsertCommand(),
						new UpdateCommand()};
		
		int idx=0;
		boolean is =true;
		
		while(is) {
			System.out.println("메뉴를 선택하세요");
			System.out.println("1:회원입력 2:회원출력 3:회원수정 4:회원삭제 5:종료");
			int idx =sc.nextInt();
		}
		
		
		sc.close();
	}
	
	
}
