package com.naver;

public class Test01 {


	//익명의 내부 클래스1
	// public 외부클래스명() { new 인터페이스명(){구현부}};	
	public Inter1 ano = new Inter1() {
		@Override
		public void me1() {
			System.out.println("가짜 익명");
		}
	};
	
	//익명의 내부 클래스2
	// public 외부클래스명() { new 인터페이스명(){구현부}};	
	// 생성자 안에 new 인터페이스(){구현부}; 와 모양이 같음
	// 이름이 없어서 따로 호출 불가
	//Test01 객체생성후 첫 메서드 호출시에만 같이 나옴
	public	Test01() {	// 외부 클래스명() 괄호가 있다.
		new Inter1() {	// new 구현 인터페이스명()
			@Override
			public void me1() {
				System.out.println("진짜 익명"); 
			}
		}.me1();;		// 여기서 바로 사용
	}
	
	public void me2() {
		System.out.println("진짜익명 호출용");
	}
	
	
}

