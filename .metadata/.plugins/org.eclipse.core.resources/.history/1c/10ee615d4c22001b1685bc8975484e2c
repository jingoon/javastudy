package com.naver;

public class Hero {
	private String name;
	Weapon[] wepons;
	Weapon w;
	public Hero() {
	
	}
	
	public Hero(String name, Weapon[] wepons, Weapon w) {
		super();
		this.name = name;
		this.wepons = wepons;
		this.w = w;
	}

	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Weapon[] getWepons() {
		return wepons;
	}
	public void setWepons(Weapon[] wepons) {
		this.wepons = wepons;
	}
	public Weapon getW() {
		return w;
	}
	public void setW(Weapon w) {
		this.w = w;
	}
	

}
